<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" text="Data Mahasiswa" />
   <TableView fx:id="TableViewMahasiswa" prefHeight="200.0" prefWidth="361.0">
     <columns>
       <TableColumn fx:id="ColumnNrp" prefWidth="75.0" text="NRP" />
       <TableColumn fx:id="ColumnFirstName" prefWidth="75.0" text="First Name" />
         <TableColumn fx:id="ColumnLastName" prefWidth="75.0" text="Last Name" />
         <TableColumn fx:id="ColumnIpk" prefWidth="75.0" text="IPK" />
     </columns>
   </TableView>
   <ButtonBar prefHeight="40.0" prefWidth="200.0">
     <buttons>
       <Button fx:id="ButtonPrev" mnemonicParsing="false" onAction="#onPrevButtonClick" text="Prev" />
          <Button fx:id="ButtonNext" onAction="#onNextButtonClick" text="Next" />
     </buttons>
   </ButtonBar>
</VBox>
